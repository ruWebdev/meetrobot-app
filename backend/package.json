        "@nestjs/testing": "^10.0.0",
        "@types/express": "^4.17.17",
        "@types/node": "^20.0.0",
        "@types/ws": "^8.5.5",
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-prettier": "^5.0.0",
        "prettier": "^3.0.3",
        "prisma": "^5.4.2",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.2.2"
